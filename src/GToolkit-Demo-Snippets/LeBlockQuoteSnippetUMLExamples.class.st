Class {
	#name : #LeBlockQuoteSnippetUMLExamples,
	#superclass : #Object,
	#category : #'GToolkit-Demo-Snippets-Examples'
}

{ #category : #example }
LeBlockQuoteSnippetUMLExamples >> blockQuoteSnippetClassHierarchy [
	<gtExample>
	^ UmlClassDiagram new
	classHierarchyFor: {
			LeSnippet. LeTextualSnippet. LeBlockQuoteSnippet.
			LeSnippetViewModel. LeTextualSnippetViewModel. LeBlockQuoteSnippetViewModel.
			LeSnippetElement. LeTextualSnippetElement. LeBlockQuoteSnippetElement
			}
	withAssociations: {
			LeBlockQuoteSnippetViewModel 
				-> {LeBlockQuoteSnippet}.
			LeBlockQuoteSnippetElement 
				-> {LeBlockQuoteSnippetViewModel}.
			}
]

{ #category : #example }
LeBlockQuoteSnippetUMLExamples >> blockQuoteSnippetClassHierarchyWithAssociations [
	<gtExample>
	^ UmlClassDiagram new
	classHierarchyFor: {
			LeSnippet. LeTextualSnippet. LeBlockQuoteSnippet.
			BlRunRopedText.
			LeSnippetViewModel. LeTextualSnippetViewModel. LeBlockQuoteSnippetViewModel.
			BrTextEditorModel .
			LeSnippetElement. LeTextualSnippetElement. LeBlockQuoteSnippetElement.
			BrEditor
			}
	withAssociations: {
			LeBlockQuoteSnippet
				-> { BlRunRopedText}.
			LeBlockQuoteSnippetViewModel 
				-> {LeBlockQuoteSnippet . BlRunRopedText . BrTextEditorModel}.
			LeBlockQuoteSnippetElement 
				-> {LeBlockQuoteSnippetViewModel . BrEditor} .
			BrEditor
				-> {BrTextEditorModel}
			}
]

{ #category : #example }
LeBlockQuoteSnippetUMLExamples >> blockQuoteSnippetClassHierarchyWithAssociationsAndLabels [
	<gtExample>
	| m |
	m :=  UmlClassDiagram new
	classHierarchyFor: {
			LeSnippet. LeTextualSnippet. LeBlockQuoteSnippet.
			BlRunRopedText.
			LeSnippetViewModel. LeTextualSnippetViewModel. LeBlockQuoteSnippetViewModel.
			BrTextEditorModel .
			LeSnippetElement. LeTextualSnippetElement. LeBlockQuoteSnippetElement.
			BrEditor
			}
	withAssociationsAndLabels: {
			LeBlockQuoteSnippet
				-> { { 'text' . BlRunRopedText }}.
			LeBlockQuoteSnippetViewModel 
				-> {{ 'model' . LeBlockQuoteSnippet} . 
				{ 'text' . BlRunRopedText} . 
				{ 'editor model' . BrTextEditorModel}}.
			LeBlockQuoteSnippetElement 
				-> {{'view model' . LeBlockQuoteSnippetViewModel} . 
				{'editor element' . BrEditor}} .
			BrEditor
				-> {{'editor model'  . BrTextEditorModel}}
			}.
		m layout custom: BlBasicLayout new.
		m root children withIndexDo: [ :child :n | child relocate: (self blockQuoteSnippetClassHierarchyWithAssociationsAndLabelsPositions at: n) ].
	^ m
]

