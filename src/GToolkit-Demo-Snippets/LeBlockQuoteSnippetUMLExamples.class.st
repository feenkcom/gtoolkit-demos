Class {
	#name : #LeBlockQuoteSnippetUMLExamples,
	#superclass : #Object,
	#category : #'GToolkit-Demo-Snippets-Examples'
}

{ #category : #example }
LeBlockQuoteSnippetUMLExamples >> blockQuoteSnippetClassHierarchyWithAssociationsAndLabels [
	<gtExample>
	| m |
	m :=  UmlClassDiagram new
	classHierarchyFor: {
			LeSnippet. LeTextualSnippet. LePlainTextSnippet . LeBlockQuoteSnippet.
				BlRunRopedText.
			LeSnippetViewModel. LeTextualSnippetViewModel. 
				LePlainTextCoderSnippetViewModel . LeBlockQuoteSnippetViewModel.
				GtTextCoder . GtTextualCoderViewModel .
			LeSnippetElement. LeTextualSnippetElement. 
				LePlainTextCoderSnippetElement . LeBlockQuoteSnippetElement.
				GtTextualCoderEditorElement
			}
	withAssociationsAndLabels: {
			LeBlockQuoteSnippet
				-> { { 'text' . BlRunRopedText }}.
			LeBlockQuoteSnippetViewModel 
				-> {
				{ 'text' . BlRunRopedText} . 
				{ 'text coder model' . GtTextCoder} .
				{ ' text coder view model' . GtTextualCoderViewModel}}.
			GtTextCoder 
				-> { 
				{ 'model' . BlRunRopedText } }.
			GtTextualCoderViewModel 
				-> { 
				{ 'coder' . GtTextCoder } }.
			LeBlockQuoteSnippetElement 
				-> {
				{'editor element' . GtTextualCoderEditorElement}} .
			GtTextualCoderEditorElement
				-> {{'editor model'  . GtTextualCoderViewModel}}
			}.
		m layout custom: BlBasicLayout new.
		m root children withIndexDo: [ :child :n | child relocate: (self positions at: n) ].
	^ m
]

