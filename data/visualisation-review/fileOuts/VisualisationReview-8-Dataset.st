Object subclass: #VRDatasetExamples	instanceVariableNames: ''	classVariableNames: ''	package: 'VisualisationReview-Examples'!!VRDatasetExamples methodsFor: 'examples' stamp: '1/29/2026 11:32'!allPapersJSON	<gtExample>	| json |	json := (CSV2JSON			for: (self datasetFolder / 'dataset-review-vissoft-all.csv') contents)			jsonObject.	self assert: json size equals: 346.	^ json! !!VRDatasetExamples methodsFor: 'examples' stamp: '1/29/2026 11:32'!datasetFolder	<gtExample>	| folder |	folder := FileLocator gtResource / 'feenkcom' / 'gtoolkit-demos'			/ 'data' / 'visualisation-review'.	self assert: folder exists.	self assert: (folder allChildrenMatching: '*.csv') size equals: 2.	^ folder! !!VRDatasetExamples methodsFor: 'examples' stamp: '1/29/2026 11:32'!selectedPapersJSON	<gtExample>	| json |	json := (CSV2JSON			for: (self datasetFolder / 'dataset-review-vissoft-selected.csv') contents)			jsonObject.	self assert: json size equals: 65.	^ json! !!VRDatasetExamples methodsFor: 'examples' stamp: '1/29/2026 11:32'!allPapers	<gtExample>	| papers |	papers := self allPapersJSON collect: [ :data | VRPaper for: data ].	self assert: papers size equals: 346.	^ papers! !!VRDatasetExamples methodsFor: 'examples' stamp: '1/29/2026 11:32'!designStudies	<gtExample>	| papers |	papers := self allPapers select: #isDesignStudy.	self assert: papers size equals: 65.	^ papers! !!VRDatasetExamples methodsFor: 'examples' stamp: '1/29/2026 11:32'!allPapersWrapped	<gtExample>	| papers |	papers := VRPaperGroup withAll: self allPapers.	self assert: papers size equals: 346.	^ papers! !!VRDatasetExamples methodsFor: 'examples' stamp: '1/29/2026 11:32'!vrDataset
	<gtExample>
	^ VRDataset
		forAllCSV: self datasetFolder / 'dataset-review-vissoft-all.csv'
		andSelectedCSV: self datasetFolder / 'dataset-review-vissoft-selected.csv'! !!VRDatasetExamples methodsFor: 'accessing' stamp: '1/29/2026 11:32'!paper128	<gtExample>	| paper |	paper := self allPapers detect: [ :p | p id = 128 ].	self assert: paper id equals: 128.	self assert: paper isDesignStudy.	^ paper! !Object subclass: #VRPaperGroup	uses: TGtGroupWithItems	instanceVariableNames: ''	classVariableNames: ''	package: 'VisualisationReview-Model'!!VRPaperGroup methodsFor: 'views' stamp: '1/29/2026 11:32'!gtItemsFor: aView	<gtView>	^ aView columnedList		title: 'Papers';		items: [ self items ];		column: 'Index'			text: [ :eachItem :eachIndex | eachIndex asRopedText foreground: Color gray ]			width: 45;		column: 'Title' text: [ :each | each title ];		column: 'Type' text: [ :each | each type ];		actionUpdateButton! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!VRPaperGroup class	uses: TGtGroupWithItems classTrait	instanceVariableNames: ''!Object subclass: #VRPaper	instanceVariableNames: 'data dataset'	classVariableNames: ''	package: 'VisualisationReview-Model'!!VRPaper methodsFor: 'accessing' stamp: '1/29/2026 11:32'!data	^ data! !!VRPaper methodsFor: 'accessing' stamp: '1/29/2026 11:32'!type	^ self data at: 'Type'! !!VRPaper methodsFor: 'accessing' stamp: '1/29/2026 11:32'!id	^ (self data at: 'ID') asNumber! !!VRPaper methodsFor: 'accessing' stamp: '1/29/2026 11:32'!title	^ self data at: 'Title'! !!VRPaper methodsFor: 'accessing' stamp: '1/29/2026 11:32'!dataset	^ dataset! !!VRPaper methodsFor: 'accessing' stamp: '1/29/2026 11:32'!dataset: aDataset	dataset := aDataset! !!VRPaper methodsFor: 'initialization' stamp: '1/29/2026 11:32'!data: aDictionary	data := aDictionary! !!VRPaper methodsFor: 'testing' stamp: '1/29/2026 11:32'!isDesignStudy	^ self type = 'Design Study'! !!VRPaper methodsFor: 'printing' stamp: '1/29/2026 11:32'!printOn: aStream	aStream nextPutAll: self title! !!VRPaper methodsFor: 'views' stamp: '1/29/2026 11:32'!gtJsonFor: aView	<gtView>	^ aView forward		title: 'JSON';		object: [ self data ];		view: #gtItemsFor:! !!VRPaper methodsFor: 'views' stamp: '1/29/2026 11:32'!gtSummaryFor: aView	<gtView>	^ aView columnedList		title: 'Summary';		priority: 10;		items: [ {{'ID'.					self id}.				{'Title'.					self title}.				{'Type'.					self type}.				{'# Authors'.					self data at: '# Authors'}.				{'# Pages'.					self data at: '# Pages'}.				{'Is selected'.					self isDesignStudy}} ];		column: 'Key'			text: #first			width: 100;		column: 'Value' text: #second;		actionUpdateButton! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!VRPaper class	instanceVariableNames: ''!!VRPaper class methodsFor: 'instance creation' stamp: '1/29/2026 11:32'!for: aDictionary	^ self new		data: aDictionary;		yourself! !!VRPaper class methodsFor: 'instance creation' stamp: '1/29/2026 11:32'!for: aDictionary in: aDataset	^ self new		data: aDictionary;		dataset: aDataset;		yourself! !Object subclass: #VRDataset	instanceVariableNames: 'allJSON selectedJSON paperDict'	classVariableNames: ''	package: 'VisualisationReview-Model'!!VRDataset methodsFor: 'initialization' stamp: '1/29/2026 11:32'!allJSON: aJson selectedJSON: anotherJson	self allJSON: aJson.	self selectedJSON: anotherJson! !!VRDataset methodsFor: 'accessing' stamp: '1/29/2026 11:32'!selectedJSON	^ selectedJSON! !!VRDataset methodsFor: 'accessing' stamp: '1/29/2026 11:32'!allJSON	^ allJSON! !!VRDataset methodsFor: 'accessing' stamp: '1/29/2026 11:32'!selectedJSON: anObject	selectedJSON := anObject! !!VRDataset methodsFor: 'accessing' stamp: '1/29/2026 11:32'!allJSON: anObject	allJSON := anObject! !!VRDataset methodsFor: 'accessing' stamp: '1/29/2026 11:35'!paperDict	^ paperDict		ifNil: [ paperDict := ((self allJSON collect: [ :d | VRPaper for: d in: self ])					collect: [ :p | p id -> p ]) asDictionary ]! !!VRDataset methodsFor: 'accessing' stamp: '1/29/2026 11:38'!allPapers	^ VRPaperGroup withAll: self paperDict values! !!VRDataset methodsFor: 'accessing' stamp: '1/29/2026 11:43'!selectedPapers	^ self allPapers select: #isDesignStudy! !!VRDataset methodsFor: 'views' stamp: '1/29/2026 11:42'!gtAllPapersFor: aView	<gtView>	^ aView forward		title: 'All papers';		priority: 10;		object: [ self allPapers ];		view: #gtItemsFor:! !!VRDataset methodsFor: 'views' stamp: '1/29/2026 11:43'!gtSelectedPapersFor: aView	<gtView>	^ aView forward		title: 'Selected papers';		priority: 20;		object: [ self selectedPapers ];		view: #gtItemsFor:! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!VRDataset class	instanceVariableNames: ''!!VRDataset class methodsFor: 'instance creation' stamp: '1/29/2026 11:32'!forAllCSV: aCSVfile andSelectedCSV: anotherCSVFile	^ self new		allJSON: (CSV2JSON for: aCSVfile contents) jsonObject		selectedJSON: (CSV2JSON for: anotherCSVFile contents) jsonObject! !